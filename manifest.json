{
  "name": "微信素材",
  "version": "1.0",
  "description": "获取微信素材中的图文消息生成XML消息包",
  "manifest_version":2,
  "browser_action": {
    "default_icon": "icon.png"     
  },
  "content_scripts": [
    {
      "matches": ["https://mp.weixin.qq.com/cgi-bin/appmsg*"],
      "js": ["jquery.js", "content_script.js"]
    }
	 ],
  "permissions": [
    "https://mp.weixin.qq.com/"
  ]
}